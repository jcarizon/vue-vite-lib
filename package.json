{
    "name": "my-vue-vite-ui-lib",
    "version": "1.2.5",
    "description": "A Vue 3 UI component library",
    "author": "JM Carizon",
    "type": "module",
    "main": "./dist/my-ui-lib.umd.js",
    "module": "./dist/my-ui-lib.es.js",
    "types": "./dist/my-ui-lib.d.ts",
    "exports": {
        ".": {
            "import": "./dist/my-ui-lib.es.js",
            "require": "./dist/my-ui-lib.umd.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite --config vite.demo.config.ts",
        "build": "vue-tsc -b tsconfig.app.json && vite build",
        "preview": "vite preview",
        "test": "vitest"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.8",
        "tailwindcss": "^4.1.8",
        "vue": "^3.5.13"
    },
    "devDependencies": {
        "@tailwindcss/postcss": "^4.1.8",
        "@types/node": "^22.15.30",
        "@vitejs/plugin-vue": "^5.2.4",
        "@vue/test-utils": "^2.4.6",
        "@vue/tsconfig": "^0.7.0",
        "autoprefixer": "^10.4.21",
        "jsdom": "^26.1.0",
        "postcss": "^8.5.4",
        "typescript": "~5.8.3",
        "vite": "^6.3.5",
        "vitest": "^3.2.2",
        "vue-tsc": "^2.2.10"
    }
}
